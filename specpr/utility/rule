#!/bin/sh

if [ -z "$1" ]
then
	echo '         11111111112222222222333333333344444444445555555555666666666677777777778'
	echo '12345678901234567890123456789012345678901234567890123456789012345678901234567890'
	exit
fi

if [ "$1" = "-40" -a -z "$2" ]
then
	echo '         1111111111222222222233333333334'
	echo '1234567890123456789012345678901234567890'
fi

if [ "$1" = "-40" -a "$2" = "-o" ]
then
	shift ; shift
	s=$1
	echo "$s"'         1111111111222222222233333333334'
	echo "$s"'1234567890123456789012345678901234567890'
fi
